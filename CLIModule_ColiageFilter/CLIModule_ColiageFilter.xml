<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Examples</category>
  <title>CLIModule_ColiageFilter</title>
  <description><![CDATA[This is a CLI module that can be bundled in an extension]]></description>
  <version>0.0.1</version>
  <documentation-url>http://www.example.com/Slicer/Modules/CLIModule_ColiageFilter</documentation-url>
  <license>Slicer</license>
  <contributor>Ahmad Algohary (Case Western Reserve University)</contributor>
  <acknowledgements>This work was partially funded by NIH-National Cancer Institute (NCI) grant CA167811</acknowledgements>
  <parameters>
    <label>IO</label>
    <description><![CDATA[Input/output parameters]]></description>

    <image>
      <name>inputVolume</name>
      <label>Input Volume</label>
      <channel>input</channel>
      <index>0</index>
      <description><![CDATA[Input volume]]></description>
    </image>
	
	<image>
      <name>inputMask</name>
      <label>Input Mask</label>
      <channel>input</channel>
      <index>1</index>
      <description><![CDATA[Input mask]]></description>
    </image>
	
    <image>
      <name>outputVolume</name>
      <label>Output Volume</label>
      <channel>output</channel>
      <index>2</index>
      <description><![CDATA[Output Volume]]></description>
    </image>
	
	<integer>
      <name>radius</name>
	  <channel>input</channel>
      <index>3</index>
      <description><![CDATA[An integer value (in units of pixels) passed to the algorithm]]></description>
      <default>2</default>
    </integer>
  </parameters>
</executable>
